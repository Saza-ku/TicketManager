@model string

@{
    var dramaName = ViewData["DramaName"];
}

<h1>一般予約一括登録</h1>
<h2>@dramaName</h2>
<form asp-action="CreateRange">
    <div class="form-group">
        <textarea class="form-control" name="input"></textarea>
    </div>
    <div class="form-group">
        <input formmethod="post" type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>

<form enctype="multipart/form-data" method="post" asp-action="CreateRange">
    <dl>
        <dt>
            CSV ファイル
        </dt>
        <dd>
            <input type="file">
        </dd>
    </dl>
    <input asp-page-handler="Upload" class="btn" type="submit" value="Upload" disabled="disabled"/>
</form>